<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane prefWidth="700.0" style="-fx-background-color: #E0F7FA;" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainPurityReportController">
    <center>
        <ListView fx:id="listViewPurityReport" style="-fx-background-color: #FFFFFF;" BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </BorderPane.margin>
        </ListView>
    </center>
    <bottom>
        <HBox prefHeight="48.0" prefWidth="600.0" BorderPane.alignment="CENTER">
            <children>
                <Button minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#mainPurityReportCancelPressed" prefHeight="30.0" prefWidth="200.0" text="Cancel">
                    <HBox.margin>
                        <Insets left="50.0" top="8.0" />
                    </HBox.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#mainPurityReportViewPressed" prefHeight="30.0" prefWidth="200.0" text="View Report">
                    <HBox.margin>
                        <Insets top="8.0" />
                    </HBox.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#mainPurityReportSubmitPressed" prefHeight="30.0" prefWidth="200.0" text="Submit New Report">
                    <HBox.margin>
                        <Insets right="50.0" top="8.0" />
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
    </bottom>
</BorderPane>
